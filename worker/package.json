{
  "name": "swimtracker-api",
  "version": "1.0.0",
  "private": true,
  "description": "Cloudflare Worker: pool scraper + JSON API",
  "scripts": {
    "dev": "wrangler dev",
    "deploy": "wrangler deploy",
    "db:create": "wrangler d1 create swimtracker",
    "db:init": "wrangler d1 execute swimtracker --file=src/schema.sql",
    "db:init:local": "wrangler d1 execute swimtracker --local --file=src/schema.sql"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4",
    "typescript": "^5",
    "wrangler": "^4"
  }
}
